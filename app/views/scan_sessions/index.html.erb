<h1>Listing scan_sessions</h1>

<table>
  <tr>
    <th>Visit</th>
    <th>User</th>
    <th>Subject</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scan_sessions.each do |scan_session| %>
  <tr>
    <td><%= scan_session.visit_id %></td>
    <td><%= scan_session.user_id %></td>
    <td><%= scan_session.subject_id %></td>
    <td><%= scan_session.location_id %></td>
    <td><%= link_to 'Show', scan_session %></td>
    <td><%= link_to 'Edit', edit_scan_session_path(scan_session) %></td>
    <td><%= link_to 'Destroy', scan_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

