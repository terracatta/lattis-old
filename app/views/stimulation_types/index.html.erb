<h1>Listing stimulation_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Interpulse interval</th>
    <th>Interburst interval</th>
    <th>Intertain interval</th>
    <th>Frequency</th>
    <th>Trains count</th>
    <th>Pulses per train</th>
    <th>Bursts per train</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stimulation_types.each do |stimulation_type| %>
  <tr>
    <td><%= stimulation_type.name %></td>
    <td><%= stimulation_type.interpulse_interval %></td>
    <td><%= stimulation_type.interburst_interval %></td>
    <td><%= stimulation_type.intertain_interval %></td>
    <td><%= stimulation_type.frequency %></td>
    <td><%= stimulation_type.trains_count %></td>
    <td><%= stimulation_type.pulses_per_train %></td>
    <td><%= stimulation_type.bursts_per_train %></td>
    <td><%= link_to 'Show', stimulation_type %></td>
    <td><%= link_to 'Edit', edit_stimulation_type_path(stimulation_type) %></td>
    <td><%= link_to 'Destroy', stimulation_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stimulation type', new_stimulation_type_path %>
